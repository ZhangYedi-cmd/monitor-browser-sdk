#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yedizhang/yedi-fe/monitor-sdk/packages/core/bin/node_modules:/Users/yedizhang/yedi-fe/monitor-sdk/packages/core/node_modules:/Users/yedizhang/yedi-fe/monitor-sdk/packages/node_modules:/Users/yedizhang/yedi-fe/monitor-sdk/node_modules:/Users/yedizhang/yedi-fe/node_modules:/Users/yedizhang/node_modules:/Users/node_modules:/node_modules:/Users/yedizhang/yedi-fe/monitor-sdk/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yedizhang/yedi-fe/monitor-sdk/packages/core/bin/node_modules:/Users/yedizhang/yedi-fe/monitor-sdk/packages/core/node_modules:/Users/yedizhang/yedi-fe/monitor-sdk/packages/node_modules:/Users/yedizhang/yedi-fe/monitor-sdk/node_modules:/Users/yedizhang/yedi-fe/node_modules:/Users/yedizhang/node_modules:/Users/node_modules:/node_modules:/Users/yedizhang/yedi-fe/monitor-sdk/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@monitor-sdk/core/bin/core.js" "$@"
else
  exec node  "$basedir/../@monitor-sdk/core/bin/core.js" "$@"
fi
